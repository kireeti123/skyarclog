{
    "log_level": "INFO",
    "formatters": ["json", "text"],
    "listeners": {
        "console": {
            "enabled": true,
            "colored": true
        },
        "file": {
            "enabled": true,
            "path": "logs/app.log",
            "rotate": true,
            "max_size": "10MB",
            "backup_count": 5
        }
    }
}
